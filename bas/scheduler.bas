P0 (DEFINE 21 MORE VARIABLES WITH COMMAND "DEFM 21")

1 GOSUB#4:C=5
2 FOR B=0 TO 21
3 IF INT D(B)=A; PRINT D(B),Z$(B):C=1
4 NEXT B: GOTO C
5 PRINT "NO SCH.": GOTO 1

P1

1 GOSUB#5
2 FOR B=0 TO 22
3 IF B>21;PRINT "MEMO OVF":GOTO 1
4 IF D(B)=0;D(B)=A:GOTO 6
5 NEXT B
6 INPUT "SCH.: ",Z$(B):GOTO 1

P2

1 PRINT "SCH.:",Z$(B),"DEL. OR NOT"
2 INPUT "D OR N ",C$:IF C$<>"D";GOTO #1
3 C=B
4 FOR B=C+1 TO 21
5 IF D(B)=0 THEN 7
6 NEXT B
7 IF B=1 THEN 9
8 D(C)=D(B-1):Z$(C)=Z$(B-1)
9 D(B-1)=0:GOTO #0

P3

1 PRINT "SCHEDULER":SET F4
2 VAC
3 GOTO#1

P4

1 INPUT "DATE ",A:RETURN

P5

1 GOSUB#4:INPUT "TIME ",B:A=A+B/10^4:RETURN
